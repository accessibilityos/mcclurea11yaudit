<ul class="large-block-grid-6 medium-block-grid-3">
  
  	{% for link in linklists.main-menu.links %}
    
  	<li class="{% if link.active%} active{% endif %}">
         <a href="{{ link.url }}">
            <span class="vac">
               <span class="va">
                  {{ link.title }}
               </span>
            </span>
         </a>
      </li>
    {% endfor %}
  
</ul>